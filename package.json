    "diff": "^5.0.0",
    "element-plus": "^1.3.0-beta.5",
    "lowdb": "^1.0.0",
    "mammoth": "^1.4.19",
    "pinia": "^2.0.11",
    "vue": "^3.2.25",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.0",
    "concurrently": "^6.5.1",
    "cross-env": "^7.0.3",
    "electron": "^16.0.5",
    "electron-builder": "^22.14.5",
    "vite": "^2.7.2",
    "wait-on": "^6.0.0"
  }
}
